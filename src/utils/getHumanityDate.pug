-
  const MONTHS = {
    1: 'янв',
    2: 'фев',
    3: 'мар',
    4: 'апр',
    5: 'май',
    6: 'июнь',
    7: 'июль',
    8: 'авг',
    9: 'сен',
    10: 'окт',
    11: 'нояб',
    12: 'дек'
  }

  const getHumanityDate = (date) => {
    const day = date.getDate();
    const month = MONTHS[date.getMonth()];
    return `${day} ${month}`;
  }